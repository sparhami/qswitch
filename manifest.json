{
  "manifest_version": 3,
  "name": "qswitch",
  "description": "An extension that provides quick switching to tabs/bookmarks",
  "version": "1.3.0",
  "background": {
    "service_worker": "src/launcher.js"
  },
  "commands": {
    "launch-switcher": {
      "suggested_key": {
        "default": "Alt+Q"
      },
      "description": "Launch qswitch"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "tabGroups",
    "bookmarks",
    "sessions"
  ]
}
