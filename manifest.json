{
  "manifest_version": 2,
  "name": "qswitch",
  "description": "An extension that provides quick switching to tabs/bookmarks",
  "version": "1.3.0",
  "background": {
    "scripts": ["src/launcher.js"],
    "persistent": false
  },
  "commands": {
    "launch-switcher": {
      "suggested_key": {
        "default": "Alt+Q"
      },
      "description": "Launch qswitch"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
    "sessions"
  ]
}
